{
  "name": "taro",
  "version": "1.0.0",
  "private": true,
  "description": "TARO3",
  "templateInfo": {
    "name": "redux",
    "typescript": false,
    "css": "sass"
  },
  "scripts": {
    "build:weapp": "cross-env TARO_PLAT=weapp taro build --type weapp",
    "build:weapptest": "cross-env TARO_PLAT=weapptest taro build --type weapp",
    "build:swan": "cross-env TARO_PLAT=swan taro build --type swan",
    "build:alipay": "cross-env TARO_PLAT=alipay taro build --type alipay",
    "build:tt": "cross-env TARO_PLAT=tt taro build --type tt",
    "build:h5": "cross-env TARO_PLAT=h5 taro build --type h5",
    "build:rn": "cross-env TARO_PLAT=rn taro build --type rn",
    "build:qq": "cross-env TARO_PLAT=qq taro build --type qq",
    "build:jd": "cross-env TARO_PLAT=jd taro build --type jd",
    "build:quickapp": "cross-env TARO_PLAT=quickapp taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:weapptest": "npm run build:weapptest -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "zip:weapp": "npm run build:weapp && gulp weapp",
    "zip:weapptest": "npm run build:weapptest && gulp weapptest"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "mirror",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@rematch/core": "^2.0.1",
    "@tarojs/components": "3.2.5",
    "@tarojs/react": "3.2.5",
    "@tarojs/runtime": "3.2.5",
    "@tarojs/taro": "3.2.5",
    "gcoord": "^0.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.3",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "we-cropper": "^1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@tarojs/cli": "3.2.5",
    "@tarojs/mini-runner": "3.2.5",
    "@tarojs/webpack-runner": "3.2.5",
    "@types/react": "^17.0.2",
    "@types/webpack-env": "^1.13.6",
    "babel-preset-taro": "3.2.5",
    "classnames": "^2.3.1",
    "cross-env": "^7.0.3",
    "dayjs": "^1.10.4",
    "del": "^6.0.0",
    "eslint": "^6.8.0",
    "eslint-config-taro": "3.2.5",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gulp": "^4.0.2",
    "gulp-zip": "^5.1.0",
    "lodash": "^4.17.21",
    "stylelint": "9.3.0",
    "taro-ui": "^3.0.0-alpha.10"
  }
}
